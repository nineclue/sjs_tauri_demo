{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "sjs_tauri_demo",
    "version": "0.1.0",
    "identifier": "com.cluenotes.sjsTauriDemo",
    "build": {
        "frontendDist": "../src"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "sjs_tauri_demo",
                "width": 800,
                "height": 600
            },
            {
                "label": "control",
                "title": "콘트롤",
                "width": 200,
                "height": 70,
                "url": "control.html",
                "parent": "main",
                "visible": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
